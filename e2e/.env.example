# E2E Test Environment Variables
# Copy to .env.staging or .env.production and fill in values

# Base URL for the environment under test
BASE_URL=https://staging.recaprabbit.com

# Test user credentials (from GitHub Secrets in CI)
E2E_USER_EMAIL=your-test-email@example.com
E2E_USER_PASSWORD=your-password-here

# Optional: Pre-created test data IDs for consistent testing
E2E_TEST_SUBSCRIPTION_ID=
E2E_TEST_EPISODE_ID=

# Set to 'true' for production to prevent mutations
E2E_READONLY=false
